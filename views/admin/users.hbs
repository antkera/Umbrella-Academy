
<ul class="list-group">
  <li class="list-group-item"><b>Lista de usuarios</b></li>

  {{#each this.arrUsers}}
  <li class="list-group-item">{{this.firstName}} <br> {{this.email}}</li>
  <div class="btn-group" role="group" aria-label="Basic outlined example">
 <a href="/admin/:{{this._id}}/detalles/"> <button type="submit" class="btn btn-outline-primary">Detail</button></a>
 <a href="/admin/:{{this._id}}/edit/"> <button type="submit" class="btn btn-outline-primary">Edit</button></a>
  <a href="/admin/:{{this._id}}/delete/"><button type="submit" class="btn btn-outline-primary">Delete</button></a>
</div>
  {{/each}}

</ul>